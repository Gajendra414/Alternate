{
  "expo": {
    "name": "Alternate",
    "slug": "Alternate",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "alternate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/icon/adaptive-icon.png",
    "ios": {
      "supportsTablet": true,
      "icon": {
        "dark": "./assets/icon/ios-dark.png",
        "light": "./assets/icon/ios-light.png",
        "tinted": "./assets/icon/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon/adaptive-icon.png",
        "monochromeImage": "./assets/icon/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./assets/icon/adaptive-icon.png",
      "edgeToEdgeEnabled": true,
      "package": "com.lulu786.Alternate"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icon/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./assets/icon/splash-icon-light.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a6216e1e-0a05-49b2-ba25-b7ea90ab8d58"
      }
    }
  }
}
